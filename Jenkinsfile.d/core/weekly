pipeline {
  agent none

  options {
    disableConcurrentBuilds()
  }

  // Every Tuesday at 4:30:00 AM Coordinated Universal Time;
  triggers {
    cron '30 4 * * 2'
  }

  stages {
    stage("Release"){
      steps {
        echo "Release build is disabled for security fixes"
      }
    }
  }
}
